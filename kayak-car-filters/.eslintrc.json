{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-one-expression-per-line": "off",
        "react/prefer-stateless-function": "off",
        "comma-dangle": ["error", "never"],
    },
    "env": {
        "browser": true,
        "node": true
    }
}
